config_version=5

[application]

config/name="fishblast"
run/main_scene="res://proto/menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GsomConsole="*res://addons/gsom_console/./gsom_console_autoload.gd"
TimeControl="*res://autoload/time_control/time_control.tscn"
DebugConsole="*res://autoload/debug_console/debug_console.tscn"
CameraControl="*res://autoload/camera_control/camera_control.tscn"

[display]

mouse_cursor/custom_image="res://assets/textures/cursor_idle_scaled.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/gsom_console/plugin.cfg")

[global_group]

menu_unpaused="Should not freeze when the menu is opened"

